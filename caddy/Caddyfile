mydata.jezz.me {
    root * /srv
    route {
        reverse_proxy /api* api:4000
        try_files {path} /index.html
        file_server
    }
}

mydatadashboard.jezz.me {
    root * /srv-dashboard
    route {
        reverse_proxy /api* dashboard-api:4001
        try_files {path} /index.html
        file_server
    }
}
